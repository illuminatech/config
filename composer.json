{
    "name": "illuminatech/config",
    "description": "Provides support for Laravel application runtime configuration managed in persistent storage",
    "keywords": ["laravel", "configuration", "config", "repository", "runtime", "persistent", "database"],
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/illuminatech/config/issues",
        "wiki": "https://github.com/illuminatech/config/wiki",
        "source": "https://github.com/illuminatech/config"
    },
    "authors": [
        {
            "name": "Paul Klimov",
            "email": "klimov.paul@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "^6.0|^7.0"
    },
    "require-dev": {
        "illuminate/database": "^6.0|^7.0",
        "illuminate/cache": "^6.0|^7.0",
        "illuminate/config": "^6.0|^7.0",
        "illuminate/encryption": "^6.0|^7.0",
        "illuminate/validation": "^6.0|^7.0",
        "psr/log": "^1.0",
        "phpunit/phpunit": "^7.5|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Illuminatech\\Config\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Illuminatech\\Config\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
